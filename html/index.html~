<script>
function getList(){
FB.api(
    "/me/",
    function (response) {
      if (response && !response.error) {
        alert(JSON.stringify(response));
document.getElementById('eventOut').innerHTML=JSON.stringify(response);
      }
    }
);}
</script>

<input type="button" value="check" name="check" onClick="getList();"/>
<div id="eventOut">test</div>
  </body>
</html>
